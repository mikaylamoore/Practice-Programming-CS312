public static void printRange(int n1, int n2){
    if (n1 > n2){
        for (int i = n1; i >= n2; i--){
            System.out.print(i + " ");
        }
    }
    else if (n1 < n2){
        for (int i = n1; i <= n2; i++){
            System.out.print(i + " ");
        }
    } else {
        System.out.print(n1);
    }
}
